      "server/node_modules",
      "react-ui/node_modules"
    ],
    "dependencies": {
      
        "firebase": "^8.6.8",
        "next": "11.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2"
      },
      "devDependencies": {
        "eslint": "7.29.0",
        "eslint-config-next": "11.0.1"
      },
     
    "repository": {
      "type": "git",
      "url": "https://github.com/thor25/grupoautobus.git"
    },
    "keywords": [
      "node",
      "heroku",
      "create-react-app",
      "react"
    ]
   
  }
  


